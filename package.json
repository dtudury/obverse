    "hash",
    "object",
    "version",
    "observe"
  ],
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dtudury/obverse/issues"
  },
  "homepage": "https://github.com/dtudury/obverse#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.8.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "rollup": "^0.49.2",
    "rollup-plugin-buble": "^0.15.0",
    "source-map-support": "^0.4.17"
  }
}
