{
  "name": "obverse",
  "version": "0.0.4",
  "description": "observable object versioning universe",
  "main": "lib/index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w -m",
    "prepublish": "npm run build",
    "pretest": "rollup -c -m",
    "test": "nyc mocha --require source-map-support/register",
    "debug": "npm run pretest ; mocha --require source-map-support/register test/temp.js",
    "lint": "eslint --ext .js src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtudury/obverse.git"
  },
  "keywords": [
